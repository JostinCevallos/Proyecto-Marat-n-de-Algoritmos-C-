#include <iostream>

int main() {
    int num1, num2, resultado; // Declarar variables para los números y el resultado

    std::cout << "Ingrese el primer número: "; // Solicitar el primer número
    std::cin >> num1; // Leer el primer número

    std::cout << "Ingrese el segundo número: "; // Solicitar el segundo número
    std::cin >> num2; // Leer el segundo número

    resultado = num1 * num2; // Realizar la multiplicación

    std::cout << "El resultado de la multiplicación es: " << resultado << std::endl; // Mostrar el resultado

    return 0;
}
