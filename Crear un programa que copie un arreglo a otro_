#include <iostream>

int main() {
    int arregloOrigen[5] = {1, 2, 3, 4, 5};
    int arregloDestino[5];

    // Copiar cada elemento del arreglo origen al arreglo destino
    for (int i = 0; i < 5; ++i) {
        arregloDestino[i] = arregloOrigen[i];
    }

    // Mostrar el arreglo copiado
    std::cout << "Arreglo copiado: ";
    for (int i = 0; i < 5; ++i) {
        std::cout << arregloDestino[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}




