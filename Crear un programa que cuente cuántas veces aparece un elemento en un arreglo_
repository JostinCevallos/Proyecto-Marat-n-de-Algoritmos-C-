#include <iostream>

int main() {
    // Definir el arreglo y el número a buscar
    int arreglo[] = {10, 20, 50, 30, 10, 10, 50, 20, 10};
    int elementoABuscar = 10;
    int contador = 0;

    // Calcular el tamaño del arreglo
    int tamano = sizeof(arreglo) / sizeof(arreglo[0]);

    // Iterar a través del arreglo para contar las apariciones
    for (int i = 0; i < tamano; ++i) {
        if (arreglo[i] == elementoABuscar) {
            contador++;
        }
    }

    // Imprimir el resultado
    std::cout << "El elemento " << elementoABuscar << " aparece " << contador << " veces." << std::endl;

    return 0;
}














