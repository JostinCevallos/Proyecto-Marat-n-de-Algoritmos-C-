#include <iostream>

int main() {
  int suma_cuadrados = 0;

  // Bucle para iterar del 1 al 10
  for (int i = 1; i <= 10; ++i) {
    // Calcula el cuadrado del nÃºmero actual y lo suma
    suma_cuadrados += i * i;
  }

  // Imprime el resultado
  std::cout << "La suma de los cuadrados de los numeros del 1 al 10 es: " << suma_cuadrados << std::endl;

  return 0;
}



